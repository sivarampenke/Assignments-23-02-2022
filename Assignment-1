# program to print minimum and maximum elements of a list
l=[]
n=int(input("Enter the value of n:"))
for i in range(1,n+1):
    x=int(input())
    l.append(x)
min=l[0]   
max=l[0]
for i in range(1,n):
    if l[i]>max:
        max=l[i]
    else:
        min=l[i]
Print(sum(l)) 
print("maximum element is: ",min)
print("minumum element is: ",max)
